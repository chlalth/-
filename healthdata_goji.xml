<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <RadioButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="365dp"
        tools:layout_editor_absoluteY="512dp" />

    <TextView
        android:id="@+id/default_action_bar"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="#C2F189"
        android:gravity="center"
        android:text="fit-fresh"
        android:textSize="25dp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-2dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/search"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.934"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/default_returnbutton"
        android:layout_width="55dp"
        android:layout_height="52dp"
        android:layout_marginStart="8dp"
        android:background="#63AC0F"
        android:foreground="@drawable/returnbutton"
        app:layout_constraintBottom_toBottomOf="@+id/default_action_bar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="296dp"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="22dp"
        android:text="다음"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="11. 충분한 휴식을 취해도 지속적으로 피로감을 느끼나요? "
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="85dp" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="330dp"
        android:layout_height="wrap_content"
        android:text="12. 과거에 비해 피부가 건조해지고 탄력이 떨어지며, 칙칙해졌다고 느끼나요? "
        tools:layout_editor_absoluteX="18dp"
        tools:layout_editor_absoluteY="138dp" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="330dp"
        android:layout_height="wrap_content"
        android:text="13. 가슴이 답답해며 불편감과 함께 가슴 통증이 발생하나요?"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="212dp" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="14. 호흡곤란을 겪은 적이 있나요? "
        tools:layout_editor_absoluteX="18dp"
        tools:layout_editor_absoluteY="268dp" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="15. 전에 비해 심장 박동이 불규칙해졌다고 느끼나요?"
        tools:layout_editor_absoluteX="18dp"
        tools:layout_editor_absoluteY="324dp" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="16. 눈꺼풀 가장자리의 살점이 노랗게 튀어나왔나요? "
        tools:layout_editor_absoluteX="18dp"
        tools:layout_editor_absoluteY="386dp" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="17. 눈의 각막 가장자리가 백색을 띠고있나요?"
        tools:layout_editor_absoluteX="18dp"
        tools:layout_editor_absoluteY="448dp" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="330dp"
        android:layout_height="wrap_content"
        android:text="18.  손등과 무릎등에 황색의 살갗이 돋아나는 발진이 있나요?"
        tools:layout_editor_absoluteX="18dp"
        tools:layout_editor_absoluteY="512dp" />

    <RadioButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="365dp"
        tools:layout_editor_absoluteY="85dp" />

    <RadioButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="365dp"
        tools:layout_editor_absoluteY="138dp" />

    <RadioButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="365dp"
        tools:layout_editor_absoluteY="212dp" />

    <RadioButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="365dp"
        tools:layout_editor_absoluteY="268dp" />

    <RadioButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="365dp"
        tools:layout_editor_absoluteY="324dp" />

    <RadioButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="365dp"
        tools:layout_editor_absoluteY="386dp" />

    <RadioButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="365dp"
        tools:layout_editor_absoluteY="448dp" />

</androidx.constraintlayout.widget.ConstraintLayout>